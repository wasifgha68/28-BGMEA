<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="614.0" prefWidth="767.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxml.pkgfor.project.UpdateProductionPlanSceneController">
   <children>
      <Button layoutX="25.0" layoutY="22.0" mnemonicParsing="false" onAction="#loadPlanOnMouseClicked" text="Load Plan" />
      <TableView fx:id="prodPlanTable" editable="true" layoutX="27.0" layoutY="105.0" prefHeight="458.0" prefWidth="714.0">
        <columns>
          <TableColumn fx:id="prodNameTable" prefWidth="175.0" text="Production Name" />
            <TableColumn fx:id="objectiveTable" prefWidth="302.0" text="Objective" />
            <TableColumn fx:id="empIDTable" prefWidth="236.0" text="Employee Assigned ID" />
        </columns>
      </TableView>
      <Button layoutX="688.0" layoutY="571.0" mnemonicParsing="false" onAction="#saveOnMouseClicked" text="Save" />
      <TextField fx:id="prodNameTextField" layoutX="25.0" layoutY="60.0" prefHeight="31.0" prefWidth="143.0" promptText="Production Name" />
      <TextField fx:id="objectiveTextField" layoutX="177.0" layoutY="60.0" prefHeight="31.0" prefWidth="143.0" promptText="Objective" />
      <TextField fx:id="employeeIDTextField" layoutX="328.0" layoutY="60.0" prefHeight="31.0" prefWidth="143.0" promptText="Employee ID" />
      <Button layoutX="485.0" layoutY="60.0" mnemonicParsing="false" onAction="#addInstanceOnMouseClicked" prefHeight="31.0" prefWidth="124.0" text="Add Instance" />
      <Button layoutX="615.0" layoutY="60.0" mnemonicParsing="false" onAction="#deleteInstanceOnMouseClicked" prefHeight="31.0" prefWidth="124.0" text="Delete Instance" wrapText="true" />
   </children>
</AnchorPane>
