         <columns>
            <TableColumn fx:id="dateTable" prefWidth="75.0" text="Date" />
            <TableColumn fx:id="itemNameTable" prefWidth="116.0" text="Item Name" />
            <TableColumn fx:id="totalSpentTable" prefWidth="170.0" text="Total Spent" />
