      <TextField fx:id="totalSpentTextField" layoutX="323.0" layoutY="53.0" prefHeight="31.0" prefWidth="146.0" promptText="Total Spent" />
      <TextField fx:id="notesTextField" layoutX="478.0" layoutY="53.0" prefHeight="31.0" prefWidth="146.0" promptText="Notes" />
